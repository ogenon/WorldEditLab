<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('partials/head'); %>
</head>
<body>
<header class="header">
    <%- include('partials/header'); %>
</header>

<div class="container mx-auto py-6 px-4">
    <h1 class="text-3xl py-4 border-b mb-10">Schematics</h1>
    <%
    var tableData = {
      columns: [
        { title: 'Name', key: 'name' },
        { title: 'Uploaded By', func: (item) => (item.uploadedBy ? item.uploadedBy.name : '-') },
        { title: 'Uploaded At', key: 'createdAt' },
        { title: '', func: (item) => (`<a href="${data.baseURL}/dl/s/${item.uuid}">Downloadlink</a>`)}
      ],
      content: JSON.parse(data.tableData)
    };
    %>
    <%- include('partials/table', tableData)%>
</div>
</body>
</html>