{
  "name": "worldeditlab",
  "version": "0.0.1",
  "description": "A fancy and simple UI for up- & downloading schematics and heightmaps",
  "main": "dist/app.js",
  "scripts": {
    "start": "node .",
    "build": "npm run build:tsc && npm run build:css",
    "build:tsc": "tsc",
    "build:css": "postcss assets/styles/tailwind.css -o public/styles/style.css",
    "lint": "npx eslint .",
    "dev": "tsc-watch -p ./src --onSuccess \"node .\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/playlegend/WorldEditLab.git"
  },
  "author": "playlegend.net",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/playlegend/WorldEditLab/issues"
  },
  "homepage": "https://github.com/playlegend/WorldEditLab#readme",
  "devDependencies": {
    "@types/ejs": "^3.0.5",
    "@types/express": "^4.17.9",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "autoprefixer": "^10.1.0",
    "eslint": "^7.16.0",
    "eslint-config-airbnb-typescript": "^12.0.0",
    "eslint-plugin-import": "^2.22.1",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.0",
    "tailwindcss": "^2.0.2",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "ejs": "^3.1.5",
    "express": "^4.17.1"
  }
}
